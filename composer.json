{
    "name": "vi-iotcustom/laravel-revisions",
    "description": "Create multiple revisions of any Eloquent model record along with its underlying relationships",
    "license": "MIT",
    "keywords": [
        "laravel",
        "revisions",
        "eloquent",
        "model",
        "relations",
        "changes",
        "copy"
    ],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Dario Vogogna",
            "email": "dario.vogogna@vargroup.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Revisions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Revisions\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\Revisions\\ServiceProvider"
            ]
        }
    }
}
